{
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "icon.png",
  "commands": [
    {
      "name": "Copy React Component",
      "identifier": "opy-svg-as-react-component",
      "script": "copy-svg-as-react-component.js",
      "shortcut": "cmd control r"
    },
    {
      "name": "Copy RN Component",
      "identifier": "copy-svg-as-rn-component",
      "script": "copy-svg-as-rn-component.js",
      "shortcut": "cmd control n"
    },
    {
      "name": "Export React Component",
      "identifier": "export-svg-with-react-component",
      "script": "export-svg-with-react-component.js",
      "handlers": {
        "actions": {
          "ExportSlices": "onExportSlices"
        }
      }
    }
  ],
  "menu": {
    "title": "sketch-svg-to-react-component",
    "items": [
      "opy-svg-as-react-component",
      "copy-svg-as-rn-component",
      "export-svg-with-react-component"
    ]
  },
  "version": "0.1.0",
  "description": "Sketch plugin to compress SVG and transform it to React (include React Native) components. By copy React component directly to you clipboard or export as files.",
  "name": "SVG to React Component",
  "identifier": "SVG to React Component",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com/reeli/sketch-svg-to-react-component/master/.appcast.xml",
  "author": "reeli",
  "authorEmail": "reeli.null@gmail.com"
}